#include <stdio.h>

void inBoi7HaiChuSo() {
  // Duyệt qua các số nguyên từ 10 đến 99
  for (int i = 10; i <= 99; i++) {
    // Kiểm tra nếu số i chia hết cho 7
    if (i % 7 == 0) {
      printf("%d ", i);
    }
  }
}

int main() {
  // Gọi hàm inBoi7HaiChuSo() để xuất các số nguyên có 2 chữ số và là bội của 7
  inBoi7HaiChuSo();

  return 0;
}
